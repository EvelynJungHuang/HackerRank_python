if __name__ == '__main__':
    x = int(input())
    y = int(input())
    z = int(input())
    n = int(input())
    
results = []
for x in range(x+1):
    for y in range(y+1):
        for z in range(z+1):
            results.append([x, y, z])

results2 =[]
for list in results:
    if sum(list) != n:
        results2.append(list)
    
print(results2)
